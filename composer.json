{
  "require": {
    "angellco/spoon": "^3.5.0",
    "carlcs/craft-redactorcustomstyles": "3.0.3",
    "craftcms/cms": "3.4.14",
    "craftcms/redactor": "2.6.1",
    "doublesecretagency/craft-matrixcolors": "2.0.1",
    "mmikkel/cp-field-inspect": "1.1.3",
    "nystudio107/craft-scripts": "^1.2",
    "rias/craft-colour-swatches": "1.2.3",
    "verbb/redactor-tweaks": "2.0.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
