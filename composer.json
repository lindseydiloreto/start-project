{
  "name": "doublesecret/starter",
  "version": "1.0.0",
  "type": "project",
  "require": {
    "carlcs/craft-redactorcustomstyles": "^3.0",
    "craftcms/cms": "^3.4",
    "craftcms/mailgun": "^1.4",
    "craftcms/redactor": "^2.6",
    "doublesecretagency/craft-cpcss": "^2.2",
    "doublesecretagency/craft-matrixcolors": "^2.0",
    "hillholliday/craft-user-manual": "^2.0",
    "mmikkel/cp-field-inspect": "^1.1",
    "nystudio107/craft-scripts": "^1.2",
    "nystudio107/craft-twigpack": "^1.2",
    "putyourlightson/craft-dashboard-begone": "^1.0",
    "verbb/knock-knock": "^1.2",
    "verbb/redactor-tweaks": "^2.0",
    "vlucas/phpdotenv": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-install-cmd": ["@craft-update"],
    "post-update-cmd": ["@craft-update"],
    "craft-update": [
      "@php craft migrate/all",
      "@php craft clear-caches/all"
    ],
    "sync": [
      "composer install && npm install --no-fund && npm run dev"
    ],
    "nuke": [
      "rm -rf vendor/ && rm composer.lock && composer clear-cache && composer update"
    ],
    "nuke-npm": [
      "npm run nuke"
    ]
  }
}
